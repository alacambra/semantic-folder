# Azure Functions runtime
AzureWebJobsStorage=UseDevelopmentStorage=true
FUNCTIONS_WORKER_RUNTIME=python

# Microsoft Graph API (App Registration)
SF_CLIENT_ID=<your-app-registration-client-id>
SF_CLIENT_SECRET=<your-app-registration-client-secret>
SF_TENANT_ID=<your-azure-ad-tenant-id>
SF_DRIVE_USER=<upn-or-object-id-of-onedrive-owner>

# Azure Key Vault
KEY_VAULT_URI=<your-keyvault-uri>

# Function App name (for deployment)
FUNCTION_APP_NAME=<your-function-app-name>
